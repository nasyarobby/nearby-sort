{
  "name": "nearby-sort",
  "version": "1.0.1",
  "description": "The library to sort array of objects by distance from a given point",
  "type": "module",
  "source": "src/index.js",
  "exports": {
    "require": "./dist/sort.cjs",
    "default": "./dist/sort.modern.js"
  },
  "main": "./dist/sort.cjs",
  "module": "./dist/sort.module.js",
  "unpkg": "./dist/sort.umd.js",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "prepare": "husky install",
    "lint": "eslint src test",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yousufkalim/nearby-sort.git"
  },
  "keywords": [
    "nearby",
    "sort",
    "geo",
    "distance",
    "finder"
  ],
  "author": "Yousuf Kalim <yousufkalim@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yousufkalim/nearby-sort/issues"
  },
  "homepage": "https://github.com/yousufkalim/nearby-sort#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "commitlint-config-non-conventional": "^1.0.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "microbundle": "^0.15.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3"
  }
}
